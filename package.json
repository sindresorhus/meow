{
	"name": "meow",
	"version": "5.0.0",
	"description": "CLI app helper",
	"main": "dist/meow.js",
	"license": "MIT",
	"repository": "sindresorhus/meow",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "sindresorhus.com"
	},
	"engines": {
		"node": ">=6"
	},
	"scripts": {
		"build": "rollup -c build/rollup.config.js",
		"lint": "xo lib test",
		"test": "yarn lint && yarn test:lib && yarn build && yarn test:dist",
		"test:lib": "ava test/lib.test.js",
		"test:dist": "ava test/dist.test.js"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"cli",
		"bin",
		"util",
		"utility",
		"helper",
		"argv",
		"command",
		"line",
		"meow",
		"cat",
		"kitten",
		"parser",
		"option",
		"flags",
		"input",
		"cmd",
		"console"
	],
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.1.0",
		"@babel/preset-env": "^7.1.0",
		"@babel/register": "^7.0.0",
		"ava": "^1.0.0",
		"babel-core": "^7.0.0-bridge.0",
		"camelcase-keys": "^4.0.0",
		"decamelize-keys": "^1.0.0",
		"esm": "^3.0.84",
		"execa": "^0.10.0",
		"indent-string": "^3.0.0",
		"loud-rejection": "^1.0.0",
		"minimist-options": "^3.0.1",
		"normalize-package-data": "^2.3.4",
		"read-pkg-up": "^3.0.0",
		"redent": "^2.0.0",
		"rollup": "^0.66.2",
		"rollup-config": "^1.0.1",
		"rollup-plugin-commonjs": "^9.1.8",
		"rollup-plugin-json": "^3.1.0",
		"rollup-plugin-license": "^0.7.0",
		"rollup-plugin-node-resolve": "^3.4.0",
		"trim-newlines": "^2.0.0",
		"xo": "*",
		"yargs-parser": "^10.0.0"
	},
	"xo": {
		"semicolon": false,
		"rules": {
			"unicorn/no-process-exit": "off"
		}
	}
}

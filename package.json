{
	"name": "meow",
	"version": "5.0.0",
	"description": "CLI app helper",
	"main": "dist/index.js",
	"license": "MIT",
	"repository": "sindresorhus/meow",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "sindresorhus.com"
	},
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"build": "rollup --config=build/rollup.config.js",
		"lint": "xo lib test",
		"prepublish": "npm run test && npm run build && npm run test:dist",
		"test": "npm run lint && npm run test:lib",
		"test:lib": "ava test/lib.test.js",
		"test:dist": "ava test/dist.test.js"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"cli",
		"bin",
		"util",
		"utility",
		"helper",
		"argv",
		"command",
		"line",
		"meow",
		"cat",
		"kitten",
		"parser",
		"option",
		"flags",
		"input",
		"cmd",
		"console"
	],
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.2.3",
		"@babel/register": "^7.0.0",
		"ava": "^1.0.1",
		"camelcase-keys": "^5.0.0",
		"decamelize-keys": "^1.1.0",
		"esm": "^3.0.84",
		"execa": "^1.0.0",
		"hard-rejection": "^1.0.0",
		"indent-string": "^3.2.0",
		"minimist-options": "^3.0.2",
		"normalize-package-data": "^2.4.0",
		"read-pkg-up": "^4.0.0",
		"redent": "^2.0.0",
		"repl": "^0.1.3",
		"rollup": "^1.0.1",
		"rollup-plugin-commonjs": "^9.2.0",
		"rollup-plugin-json": "^3.1.0",
		"rollup-plugin-node-resolve": "^4.0.0",
		"trim-newlines": "^2.0.0",
		"xo": "^0.23.0",
		"yargs-parser": "^11.1.1"
	},
	"xo": {
		"rules": {
			"unicorn/no-process-exit": "off",
			"ava/no-ignored-test-files": "warn"
		}
	}
}
